<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/bghome"
    tools:context="com.example.asus.hujayapps.IqroDuaActivity">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/iqro_kedua"
        android:textStyle="bold"
        android:textColor="#ffffff"
        android:textSize="16sp"
        android:layout_marginTop="42dp"
        android:id="@+id/textView"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/hujay_apps"
        android:textColor="#ffffff"
        android:textSize="12sp"
        android:layout_below="@+id/textView"
        android:layout_centerHorizontal="true"
        android:id="@+id/textView2" />

    <ImageButton
        android:layout_width="150dp"
        android:layout_height="170dp"
        android:background="@drawable/teoridua"
        android:onClick="moveduateori"
        tools:layout_editor_absoluteY="132dp"
        tools:layout_editor_absoluteX="16dp"
        android:id="@+id/imageButton11"
        android:layout_marginBottom="29dp"
        android:layout_above="@+id/imageButton10"
        android:layout_alignLeft="@+id/imageButton10"
        android:layout_alignStart="@+id/imageButton10" />

    <ImageButton
        android:layout_width="150dp"
        android:layout_height="170dp"
        android:background="@drawable/nulisdua"
        android:onClick="moveduanulis"
        tools:layout_editor_absoluteY="132dp"
        tools:layout_editor_absoluteX="186dp"
        android:id="@+id/imageButton12"
        android:layout_alignTop="@+id/imageButton11"
        android:layout_toRightOf="@+id/imageButton11"
        android:layout_toEndOf="@+id/imageButton11"
        android:layout_marginLeft="16dp"
        android:layout_marginStart="16dp" />

    <ImageButton
        android:layout_width="150dp"
        android:layout_height="170dp"
        android:background="@drawable/qadua"
        android:onClick="moveqakedua"
        tools:layout_editor_absoluteY="314dp"
        tools:layout_editor_absoluteX="16dp"
        android:layout_alignTop="@+id/imageButton10"
        android:layout_alignLeft="@+id/imageButton12"
        android:layout_alignStart="@+id/imageButton12" />

    <ImageButton
        android:layout_width="150dp"
        android:layout_height="170dp"
        android:background="@drawable/gamedua"
        android:onClick="movegamekedua"
        tools:layout_editor_absoluteY="314dp"
        tools:layout_editor_absoluteX="186dp"
        android:id="@+id/imageButton10"
        android:layout_alignParentBottom="true"
        android:layout_alignRight="@+id/textView2"
        android:layout_alignEnd="@+id/textView2"
        android:layout_marginRight="31dp"
        android:layout_marginEnd="31dp"
        android:layout_marginBottom="83dp" />


</RelativeLayout>
